{
  "name": "stylelint-plugin-aurora",
  "version": "2.0.0",
  "description": "Various Stylelint rules in one plugin.",
  "files": [
    "dist",
    "docs"
  ],
  "type": "module",
  "main": "./dist/index.js",
  "directories": {
    "doc": "docs",
    "test": "tests"
  },
  "scripts": {
    "release": "release-it",
    "lint": "eslint --fix './**/*.{js,mjs,ts}'",
    "prepack": "unbuild",
    "test": "cross-env NODE_OPTIONS='--experimental-vm-modules' jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CyanSalt/stylelint-plugin-aurora.git"
  },
  "bugs": {
    "url": "https://github.com/CyanSalt/stylelint-plugin-aurora/issues"
  },
  "homepage": "https://github.com/CyanSalt/stylelint-plugin-aurora#readme",
  "keywords": [
    "stylelint",
    "stylelint-plugin"
  ],
  "author": "CyanSalt",
  "license": "ISC",
  "devDependencies": {
    "@cyansalt/eslint-config": "^4.0.0",
    "@cyansalt/tsconfig": "^2.2.1",
    "@release-it/conventional-changelog": "^8.0.1",
    "@swc/jest": "^0.2.36",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.12.12",
    "cross-env": "^7.0.3",
    "eslint": "^9.3.0",
    "jest": "^29.7.0",
    "jest-preset-stylelint": "^7.0.0",
    "release-it": "^17.0.0",
    "stylelint": "^16.5.0",
    "typescript": "^5.3.3",
    "unbuild": "^3.2.0"
  },
  "peerDependencies": {
    "stylelint": ">=16.0.0"
  }
}
